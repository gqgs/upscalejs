{
  "name": "upscalejs",
  "version": "0.2.1",
  "files": ["dist"],
  "types": "./dist/image/worker.d.ts",
  "description": "Upscale images using super resolution AI models",
  "main": "./dist/upscalejs.umd.js.",
  "module": "./dist/upscalejs.es.js",
  "exports": {
    ".": {
      "import": "./dist/upscalejs.es.js",
      "require": "./dist/upscalejs.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lib": "vue-tsc --noEmit && vite build -c vite.lib.config.ts && vue-tsc -d --emitDeclarationOnly --outDir dist",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "keywords": [
    "image",
    "upscale",
    "ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gqgs/upscalejs.git"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "onnxruntime-web": "^1.11.0",
    "prettier": "^2.5.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.1",
    "vue-tsc": "^0.33.9"
  }
}
